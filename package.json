{
    "name": "Habitat",
    "description": "Home Surveillance",
    "version": "0.0.1",
    "dependencies": {
        "node-red-contrib-image-output": "0.6.3",
        "node-red": "1.x",
        "node-red-contrib-nexmo": "3.3.1",
        "node-red-contrib-tensorflow": "0.2.2",
        "node-red-dashboard": "3.2.3",
        "node-red-node-email": "1.18.2",
        "node-red-node-ui-webcam": "0.4.0",
        "node-red-contrib-sms-free-mobile": "0.1.0"
    },
    "node-red": {
        "settings": {
            "flowFile": "flows.json",
            "credentialsFile": "flows_cred.json"
        }
    },
    "scripts": {
        "start": "node --max-old-space-size=256 ./node_modules/node-red/red.js --userDir . --settings ./settings.js flows.json"
    }
}